{
  "name": "blather",
  "version": "1.1.0",
  "private": true,
  "proxy": "http://localhost:8888/blather/",
  "scripts": {
    "build-semantic": "cd src/semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/semantic && yarn run build-semantic && gulp watch",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "prettify": "prettier --print-width 100 --tab-width 4 --use-tabs true --no-semi true --trailing-comma none --jsx-bracket-same-line false --parser flow --write 'src/!(semantic)/**/*.js'",
    "start": "react-scripts start",
    "start-server": "node server/bootstrap.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-preset-react-app": "^7.0.2",
    "depcheck": "^0.7.2",
    "dom-confetti": "^0.1.1",
    "express": "^4.17.1",
    "history": "^4.9.0",
    "html-react-parser": "^0.6.4",
    "html2canvas": "^1.0.0-alpha.12",
    "ignore-styles": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "linkifyjs": "^2.1.9",
    "lodash": "^4.17.15",
    "marked": "^0.6.3",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.26",
    "node-sass-chokidar": "^1.3.5",
    "numeral": "^2.0.6",
    "prettier": "^1.18.2",
    "query-string": "^6.8.3",
    "react": "^16.9.0",
    "react-color": "^2.17.3",
    "react-copy-to-clipboard": "^5.0.1",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.9.0",
    "react-dropzone": "^8.2.0",
    "react-facebook": "^8.0.4",
    "react-facebook-login": "^4.1.1",
    "react-ga": "^2.6.0",
    "react-google-login": "^5.0.5",
    "react-grid-gallery": "^0.5.5",
    "react-image-lightbox": "^5.1.0",
    "react-image-masonry": "^0.4.0",
    "react-linkify": "^0.2.2",
    "react-meta-tags": "^0.7.4",
    "react-moment": "^0.8.4",
    "react-number-format": "^4.2.0",
    "react-player": "^1.12.0",
    "react-redux": "^6.0.1",
    "react-redux-loading-bar": "^4.4.0",
    "react-router-dom": "^4.4.0-beta.8",
    "react-router-scroll-top": "^0.2.1",
    "react-scripts": "2.1.3",
    "react-semantic-ui-range": "^0.7.0",
    "react-share": "^2.4.0",
    "react-simple-timefield": "^2.0.3",
    "react-svg": "^7.2.14",
    "react-text-truncate": "^0.13.1",
    "react-toastify": "^5.5.0",
    "react-tooltip": "^4.2.8",
    "react-twitter-widgets": "^1.7.1",
    "react-typist": "^2.0.5",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
